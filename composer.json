{
  "name": "ivan-grebnev/laravel-rabbitmq",
  "description": "Laravel queue RabbitMQ driver.",
  "keywords": ["laravel", "rabbitmq"],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/ivan-grebnev/laravel-rabbitmq/issues",
    "source": "https://github.com/ivan-grebnev/laravel-rabbitmq"
  },
  "authors": [
    {
      "name": "Ivan Grebnev",
      "email": "9webdev@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "illuminate/queue": "^8.64",
    "php-amqplib/php-amqplib": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "IvanGrebnev\\LaravelRabbitmq\\": "src//"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "IvanGrebnev\\LaravelRabbitmq\\RabbitmqServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
